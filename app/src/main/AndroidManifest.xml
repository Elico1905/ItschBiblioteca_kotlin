        android:supportsRtl="true"
        android:theme="@style/Theme.ITSCHBiblioteca">
        <activity android:name=".Admin.Activity_AdminViewPager"></activity>
        <activity android:name=".RecyclerAdmin.Admin" />
        <activity
            android:name=".objetos.SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@style/Splassh">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecyclerAlumnos.Mensajes" />
        <activity android:name=".Menu" />
        <activity android:name=".Registro" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity" />


        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>